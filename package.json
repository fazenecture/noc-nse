{
  "name": "noc-nse",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config build/index.js",
    "start:dev": "DOTENV_CONFIG_PATH=.env.local nodemon -r dotenv/config --watch \"src/**/*.ts\" --exec \"ts-node\" --files \"src/index.ts\" --signal SIGKILL",
    "copy-files": "copyfiles -u 1 ./src/**/*.html build/src/",
    "build": "rm -rf build && tsc -p .",
    "dev": "NODE_ENV=development concurrently \"yarn start:dev\"",
    "predeploy:prod": "./scripts/predeploy.sh && yarn build",
    "deploy:prod": "NODE_ENV=production node build/index.js",
    "pretty": "prettier --write ./src/**/*.ts",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "deploy:dev": "tsc && node build/src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.1",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-cron": "^4.0.6",
    "nodemon": "^3.1.9",
    "pg": "^8.14.1",
    "puppeteer": "^24.9.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sqlutils": "^1.5.4",
    "tsc": "^2.0.4",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@types/pg": "^8.11.11",
    "esbuild": "^0.25.2",
    "serverless-esbuild": "^1.55.0"
  }
}
